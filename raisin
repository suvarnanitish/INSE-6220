import pandas as pd
import csv

# We need to install this as a dependency to process Excel files
# pip install xlrd

fn = "Raisin Data.xls"

# Convert the Excel file to CSV and save so we can process it
data_xls = pd.read_excel(fn, 'Page 1', index_col=None)
data_xls.to_csv(f'{fn}.csv', encoding='utf-8')
  
# Open CSV file
file = open(f'{fn}.csv', encoding='ISO-8859-1')
reader = csv.reader(file, delimiter=',', quotechar='"')
  
# Go through each row of the CSV
for row in reader:
  for rows in reader:
    print(rows)
   
